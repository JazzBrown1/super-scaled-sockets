{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "object-shorthand": "off",
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "max-len": "off",
    "no-param-reassign": "off"
  }
}